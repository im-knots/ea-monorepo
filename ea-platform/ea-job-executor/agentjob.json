{
    "name": "color picker",
    "creator": "1f4df0fa-655a-4ac6-b4bf-590abd6a4b5b",
    "description": "An example agent using the Ollama LLM definition.",
    "nodes": [
        {
            "alias": "color1",
            "type": "worker.inference.llm.ollama",
            "parameters": {
                "model": "llama3.2",
                "prompt": "{{in.input}}",
                "stream": false,
                "temperature": "0.7"
            }
        },
        {
            "alias": "color2",
            "type": "worker.inference.llm.ollama",
            "parameters": {
                "model": "llama3.2",
                "prompt": "{{in.input}}",
                "stream": false,
                "temperature": "0.7"
            }
        },
        {
            "alias": "in",
            "type": "input.internal.text",
            "parameters": {
                "input": "pick a color and only output that color name and nothing else"
            }
        },
        {
            "alias": "color1-response",
            "type": "input.internal.text",
            "parameters": {
                "input": "{{color1.response}}"
            }
        },
        {
            "alias": "color2-response",
            "type": "input.internal.text",
            "parameters": {
                "input": "{{color2.response}}"
            }
        },
        {
            "alias": "picker-prompt",
            "type": "input.internal.text",
            "parameters": {
                "input": "//prompt: Pick one of these colors and tell me why//"
            }
        },
        {
            "alias": "picker",
            "type": "worker.inference.llm.ollama",
            "parameters": {
                "model": "llama3.2",
                "prompt": "{{picker-prompt.input}}",
                "stream": false,
                "temperature": "0.7"
            }
        }
    ],
    "edges": [
        {
            "from": [
                "in"
            ],
            "to": [
                "color1"
            ]
        },
        {
            "from": [
                "in"
            ],
            "to": [
                "color2"
            ]
        },
        {
            "from": [
                "color1"
            ],
            "to": [
                "color1-response"
            ]
        },
        {
            "from": [
                "color2"
            ],
            "to": [
                "color2-response"
            ]
        },
        {
            "from": [
                "picker-prompt"
            ],
            "to": [
                "picker"
            ]
        },
        {
            "from": [
                "color1-response"
            ],
            "to": [
                "picker"
            ]
        },
        {
            "from": [
                "color2-response"
            ],
            "to": [
                "picker"
            ]
        }
    ]
}