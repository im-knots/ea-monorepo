{
    "name": "Compare a story from two different models",
    "creator": "<SOME USER UUID>",
    "description": "CREATED WITH AGENT BUILDER :)",
    "nodes": [
        {
            "alias": "ollama",
            "type": "worker.inference.llm.ollama",
            "parameters": {
                "model": "llama3.2",
                "prompt": "{{in.input}}",
                "stream": false,
                "temperature": "0.7"
            }
        },
        {
            "alias": "deepseek",
            "type": "worker.inference.llm.ollama",
            "parameters": {
                "model": "deepseek-r1:8b",
                "prompt": "{{in.input}}",
                "stream": false,
                "temperature": "0.7"
            }
        },
        {
            "alias": "in",
            "type": "input.internal.text",
            "parameters": {
                "input": "tell me a story about space and time"
            }
        },
        {
            "alias": "compare",
            "type": "worker.inference.llm.ollama",
            "parameters": {
                "model": "llama3.2",
                "prompt": "{{compare-prompt.input}}",
                "stream": false,
                "temperature": "0.7"
            }
        },
        {
            "alias": "ollama-out",
            "type": "input.internal.text",
            "parameters": {
                "input": "{{ollama.response}}"
            }
        },
        {
            "alias": "deepseek-out",
            "type": "input.internal.text",
            "parameters": {
                "input": "{{deepseek.response}}"
            }
        },
        {
            "alias": "compare-prompt",
            "type": "input.internal.text",
            "parameters": {
                "input": "USERPROMPT: COMPARE THESE TWO STORIES AND PICK ONE AND TELL ME WHY YOU PICKED IT END USER PROMPT"
            }
        },
        {
            "alias": "out",
            "type": "destination.internal.text",
            "parameters": {
                "input": "{{compare.response}}"
            }
        }
    ],
    "edges": [
        {
            "from": [
                "in"
            ],
            "to": [
                "ollama"
            ]
        },
        {
            "from": [
                "in"
            ],
            "to": [
                "deepseek"
            ]
        },
        {
            "from": [
                "deepseek"
            ],
            "to": [
                "deepseek-out"
            ]
        },
        {
            "from": [
                "deepseek-out"
            ],
            "to": [
                "compare"
            ]
        },
        {
            "from": [
                "compare-prompt"
            ],
            "to": [
                "compare"
            ]
        },
        {
            "from": [
                "ollama-out"
            ],
            "to": [
                "compare"
            ]
        },
        {
            "from": [
                "compare"
            ],
            "to": [
                "out"
            ]
        },
        {
            "from": [
                "ollama"
            ],
            "to": [
                "ollama-out"
            ]
        }
    ]
}