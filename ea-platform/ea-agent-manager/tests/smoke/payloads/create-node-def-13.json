{
    "type": "utils.external.github.pr",
    "name": "Github Pull Request",
    "creator": "<UUID OF CREATOR USER>",
    "api": {
      "base_url": "https://api.github.com",
      "endpoint": "/repos/{owner}/{repo}/pulls",
      "method": "POST",
      "headers": {
        "Accept": "application/vnd.github+json",
        "Authorization": "Bearer ((github_api_key))",
        "X-Github-Api-Version": "2022-11-28"
      }
    },
    "parameters": [
      {
        "key": "owner",
        "type": "string",
        "description": "The github owner of the repo",
        "default": "testuser"
      },
      {
        "key": "repo",
        "type": "string",
        "description": "The name of the github repo",
        "default": "somerepo"
      },
      {
        "key": "title",
        "type": "string",
        "description": "The title of the pull request",
        "default": "GENERATED BY EA PLATFORM"
      },
      {
        "key": "body",
        "type": "string",
        "description": "The body of the pull request",
        "default": "AI GENERATED - Review carefully!"
      },
      {
        "key": "head",
        "type": "string",
        "description": "The feature branch for the pull request",
        "default": "somefeaturebranch"
      },
      {
        "key": "base",
        "type": "string",
        "description": "The base branch for the pull request to merge into",
        "default": "main"
      }
    ],
    "outputs": [
      {
        "key": "textoutput",
        "type": "string",
        "description": "The weather forecast data."
      }
    ],
    "metadata": {
      "description": "Creates a github pull request",
      "tags": ["utils", "github", "api", "external", "pr", "pull request"],
      "additional": {
        "documentation_url": "https://docs.github.com/en/rest/pulls/pulls?apiVersion=2022-11-28"
      }
    }
}
  